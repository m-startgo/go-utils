{
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.patterns": {
    ".gitignore": ".gitattributes, .gitmodules, .stignore, ",
    "go.mod": "go.sum",
    "README.md": "AGENTS.md, LICENSE",
    "package.json": "*.json, go.mod, config.*, *config.ts, eslint*, .prettier*, *.d.ts, *lock, *json.md5"
  },
  "[go]": {
    "editor.defaultFormatter": "golang.go"
  },
  "[go.mod]": {
    "editor.defaultFormatter": "golang.go"
  },
  "gopls": {
    "formatting.gofumpt": true,
    "ui.semanticTokens": true
  },
  "files.eol": "\n",
  "editor.formatOnSave": true,
  "editor.tabSize": 2,
  "files.exclude": {
    "node_modules": true
  },
  "cSpell.words": [
    "gnet",
    "mcron",
    "mcycle",
    "mfile",
    "mhttp",
    "Millis",
    "mjson",
    "mlog",
    "mmath",
    "mpath",
    "mstr",
    "mudp",
    "mverify",
    "Println",
    "serv",
    "startgo",
    "panjf2000"
  ]
}
